<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:locals="clr-namespace:WSB_rent_car_app"
            xmlns:models="clr-namespace:WSB_rent_car_app.Models"
            x:Class="WSB_rent_car_app.MainApplicationWindow"
            NavigationPage.HasNavigationBar="false"
            BarBackgroundColor="White"
            BarTextColor="Gray">
    <TabbedPage.BindingContext>
        <models:UserDataModel/>
    </TabbedPage.BindingContext>
    <locals:CarSearch Title="Search" Icon="search.png"/>
    <locals:CarRentHistory Title="History" Icon="history.png" />
    <locals:UserData Title="User" Icon="person.png"/>
</TabbedPage>